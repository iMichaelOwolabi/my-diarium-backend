{
  "name": "my-diarium-backend",
  "version": "1.0.0",
  "description": "My Diarium is something like a journal where users make a record of their daily activities which we then refer to as memories",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha test/*.js --require @babel/register --exit",
    "build": "babel-node src -d lib",
    "serve": "node lib/app.js",
    "start": "npm run build && npm run serve",
    "dev": "nodemon --exec babel-node src/app.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iMichaelOwolabi/my-diarium-backend.git"
  },
  "author": "Michael Owolabi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iMichaelOwolabi/my-diarium-backend/issues"
  },
  "homepage": "https://github.com/iMichaelOwolabi/my-diarium-backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "apollo-server-express": "^2.8.2",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "supertest": "^4.0.2"
  }
}
